{
  "name": "drenso/genphrase",
  "description": "Secure passphrase generator for PHP applications",
  "keywords": ["secure", "random","password","passphrase","generator","diceware"],
  "homepage": "https://github.com/timoh6/GenPhrase",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.3",
    "ext-bcmath": "*",
    "ext-mbstring": "*",
    "ext-random": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.75.0",
    "phpstan/phpstan": "2.1.16",
    "phpunit/phpunit": "11.5.20",
    "rector/rector": "2.0.16"
  },
  "autoload": {
    "psr-4": {
      "Drenso\\GenPhrase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drenso\\GenPhrase\\Tests\\": "tests/phpunit/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
